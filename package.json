{
  "name": "myshop",
  "version": "1.0.0",
  "description": "MERN shopping app for practice",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "export PORT=8081 && react-scripts server",
    "server": "export PORT=8081 && nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "shop": "concurrently \"npm run server\" \"npm run client\""
