{
  "name": "myshop",
  "version": "1.0.0",
  "description": "MERN shopping app for practice",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "shop": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrieaZon/MyShop.git"
  },
  "author": "Joshua Delao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TrieaZon/MyShop/issues"
  },
  "homepage": "https://github.com/TrieaZon/MyShop#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.4",
    "nodemon": "^3.0.2"
  }
}
